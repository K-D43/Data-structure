# Data-structure
class Rotate {
    public static void rotate(int arr[]) {
        int n = arr.length;
        int x = arr[n - 1];
        for (int i = arr.length-1; i > 0; i--)
            arr[i] = arr[i - 1];
        arr[0] = x;
    }

    public static void main(String[] args) {
     int arr[]={10,20,30,40,50};
     rotate(arr);
        System.out.println(Arrays.toString(arr));
    }
}
